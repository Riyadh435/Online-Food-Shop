      <VBox fx:id="vbox" layoutX="12.0" layoutY="195.0" prefHeight="44.0" prefWidth="413.0" />
      <Label fx:id="post_title" layoutX="23.0" layoutY="112.0" prefHeight="28.0" prefWidth="198.0" text="Title">

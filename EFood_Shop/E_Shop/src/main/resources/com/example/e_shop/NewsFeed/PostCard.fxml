<AnchorPane fx:id="main_form" prefHeight="252.0" prefWidth="433.0" stylesheets="@PostCard.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.e_shop.NewsFeed.PostCard">
